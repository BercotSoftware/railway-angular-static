{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "railway-build": "npm run railway-config && ng build --configuration=railway",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "download-spec": "curl -o play-golf-api.yaml http://localhost:4000/api-docs.yaml",
    "compile-spec": "rimraf src/play-golf-api && npx openapi-generator-cli generate -i play-golf-api.yaml -g typescript-angular -o src/play-golf-api",
    "update-spec": "npm run download-spec && npm run compile-spec",
    "railway-config": "npx envsub scripts/environment.railway.ts.template src/environments/environment.railway.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "bootstrap": "^5.3.3",
    "jquery": "^3.7.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular/cli": "^17.0.3",
    "@angular/compiler-cli": "^17.0.0",
    "@openapitools/openapi-generator-cli": "^2.11.0",
    "@types/jasmine": "~5.1.0",
    "envsub": "^4.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.2.2"
  }
}
